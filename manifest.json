{
  "manifest_version": 3,
  "name": "Political Bias Analyzer",
  "version": "1.0",
  "description": "Max's tweak checker",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["popup.css"]
    }
  ]
}
